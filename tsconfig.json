{
    "compilerOptions":{
        "noLib": false,
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "outDir": "./javascript/"
    },
    "include": ["src/**/*.ts"],
    "exclude":[
        "node_modules",
        "typings",
        "wwwroot",
        "javascript",
        "dist",
        "src/.baseDir.ts"
    ]
}